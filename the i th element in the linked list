//the ith element in the linked list
public static void printIth(LinkedListNode<Integer> head, int i){
	
        int count=0;
        while(head!=null)
        {
            
            if(count==i)
            {
                System.out.println(head.data);
                break;
            }
            count++;
            head=head.next;
                
        }
		
	}
